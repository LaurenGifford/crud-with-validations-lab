<h1>All The Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th>
            <th>Genre</th>
            <th>Year</th>
            <th>Released?</th>
        </tr>
    </thead>

    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
                <td><%= song.release_year %></td>
                <td><%= song.released? %></td>
                <td></td>
            </tr>
        <% end %>
    </tbody>
</table>